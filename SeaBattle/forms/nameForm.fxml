    <Button fx:id="btnOk" mnemonicParsing="false" onAction="#enterNameAction" text="OK" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
    <TextField fx:id="enterName" prefWidth="148.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <GridPane.margin>
        <Insets left="50.0" right="50.0" />
      </GridPane.margin>
    </TextField>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Your name:" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
